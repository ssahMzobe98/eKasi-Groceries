{
  "name": "ekasi/groceries",
  "type":"project",
  "description": "This is my PHP project.",
  "require": {
    "php": "^8.2",
    "ext-mysqli":"*"
  },
  "license": "MIT",
  "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Controller\\": "controller/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3"
  }
}
